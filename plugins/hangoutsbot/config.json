{
  "commands_enabled": true, 
  "commands_aliases": ["/sciz"],
  "spawn": {
    "commands": {
      "req": {
        "command" : ["/usr/bin/python", "/path/to/sciz.py",  "-c",  "/path/to/confs/sciz.ini",  "-r"],
        "allow_args": true
      }
